{
  "schemaVersion": 1,
  "id": "genshinui",
  "version": "0.0.2-alpha2",
  "name": "Genshin UI",
  "description": "A minecraft mod brings Genshin Impact's UI experience to minecraft",
  "authors": [
    "jack253-png"
  ],
  "contact": {
    "homepage": "https://github.com/Jack253-png/BetterUI",
    "issues": "https://github.com/Jack253-png/BetterUI/issues"
